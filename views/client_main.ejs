    <script>
      $('#logoutBtn').click(function(){
        $.ajax({
				url: '/logout',
				type: 'GET',
				success:function(result) {
          alert("success")
          location.href = "/login"
				},	
				error:function(error) {
					console.log("error : " + error)	
				}
			  })
      })

      var parameterKey = new Array()

      $('.submit').click( function() {

	      //파라미터값 초기화
      	var parameterValue = ''

	      for(var i=0 ; i < parameterKey.length ; i++){
          if($('#'+parameterKey[i]).attr('type') == 'text'){
	          parameterValue += parameterKey[i] + '=' + $('#'+parameterKey[i]).val()+' '
          }else if ($('#'+parameterKey[i]).attr('type') == 'radio' || $('#'+parameterKey[i]).attr('type') == 'checkbox'){
	
	          var checkLists = []

	          parameterValue += parameterKey[i] + '='
	          $('input[name="'+parameterKey[i]+'/"]:checked').each(function(j){
		         checkLists.push($(this).val())
	          })

	          for(var j = 0 ; j < checkLists.length ; j++){
  		        parameterValue += checkLists[j] + ' '
	          }
          }
        }
      
        console.log(parameterValue)

        //예외처리
        if($('#select_data option:selected').val() == null){
          alert('please select data file')
        } else if ($('#select_APP option:selected').val() == null){
          alert('please select Application')
        } else{
          $('.result').html('prosessing');
          $.ajax({
            url: '/client/sparkSubmit',
            method: 'post',
            data: {
				    	'APP' : $('#select_APP option:selected').val(),
				    	'data' : $('#select_data option:selected').val(),
				    	'parameter' : parameterValue,
					    'target': $('input[name="sendRadio"]:checked').val(),
					    'user' : $('#user').val()
				    },
            dataType: 'json',
            success: function(result) {
              if(result){
                $('.result').html(result.result);
              }
            }
          })
          }
        })
        $('.delete').click( function() {
          if($('#select_data option:selected').val() == null){
            alert('please select data file')
          } else {
            $.ajax({
            url: '/client/dataDelete',
            method: 'post',
            data: {
	            'data' : $('#select_data option:selected').val()
	          },
            dataType: 'json',
            success: function(result) {
              if(result){
          	    $('#select_data').empty();
	              for(var i=1 ; i<result.datalist.length ; i++){
                  $('#select_data').append('<option value='+result.datalist[i]+'>'+result.datalist[i]+'</option>');
	              }
              }
            }
            });
          }
        });
      $(document).ready(function(){
        $.ajax({
          url: '/client/makeList',
          method: 'post',
          success: function(result) {
            if(result){
	            for(var i=0 ; i<result.applist.length ; i++){
                $('#select_APP').append('<option value='+result.applist[i]+'>'+result.applist[i]+'</option>');
	            }
	          }
	        }
        });
        $.ajax({
          url: '/client/makeList',
          method: 'post',
          success: function(result) {
            if(result){
        	    $('#select_data').empty();
	            for(var i=1 ; i<result.datalist.length ; i++){
                $('#select_data').append('<option value='+result.datalist[i]+'>'+result.datalist[i]+'</option>');
	            }
            }
          }
        });
	      updateLog()
      });

      function updateLog(){
	      $.ajax({
	        url: '/client/sparkLog',
	        method: 'post',
	        success:function(result){
	          if(result){
		          $('#sparkLog').val(result.latelyLog)
	          }
	        }
	      })
	      setTimeout('updateLog()',1000)
      }

      $('#fileForm').change(function(){
	      var form = $('#fileForm')[0];
	      var formData = new FormData(form);

	      if($('#fileInput').val() == ""){
          alert('please select upload file')
        } else {	
	        $.ajax({
	          url:'/client/dataUpload',
	          processData: false,
	          contentType: false,
	          data : formData,
	          type: 'POST',
	          success: function(result){
		          $('#select_data').empty();
		          for(var i=1 ; i<result.datalist.length ; i++){
		            $('#select_data').append('<option value='+result.datalist[i]+'>'+result.datalist[i]+'</option>');
		          }
	          }
	        });
	      }
      });
      $('#select_APP').click( function() {
        $.ajax({
          url: '/client/makeParameterBlank',
          method: 'post',
          data: {
	          'appname' : $('#select_APP option:selected').val()
	        },
          dataType: 'json',
          success: function(result) {
            if(result){
        	    //파라미터 입력 공간 초기화
	            $('#parameter_blank').empty();
	            parameterKey = []

	            //모든 파라미터 체크
	            for(var i=0 ; i<result.nameList.length ; i++){

	              //파라미터 키 리스트 작성
	              parameterKey[i] = result.nameList[i]

	              //파라미터 입력 타입 체크
	              if(result.typeList[i] == 'text'){
		            //text 방식 입력 공간 생성
  		            $('#parameter_blank').append(
		              result.nameList[i]+' '+'<input id='+result.nameList[i]+' type="text" maxlength="'+result.typeMaxList[i]+'"> ['+result.descriptionList[i]+']'+'<br>'
		              )
	              }else if(result.typeList[i] == 'radio' || result.typeList[i] == 'checkbox'){		//make radio or checkbox blank
  		            $('#parameter_blank').append(result.nameList[i]+' ['+result.descriptionList[i]+']'+'<br>')
		              for(var j = 0 ; j < result.typeDataList[i].length ; j++){
  		              $('#parameter_blank').append(
		                '<input id='+result.nameList[i]+' type="'+result.typeList[i]+'" value='+result.typeDataList[i][j]+' name='+result.nameList[i]+'/>'+result.typeDataList[i][j]+'<br>'
		                )
		              }
	              }
    	        }
	            $('#parameter_blank').append('<br>'+'Appname : '+result.appname)
	            $('#parameter_blank').append('<br>'+'App Version : '+result.version)
	            $('#parameter_blank').append('<br>'+'Description : '+result.description)
	            $('#parameter_blank').append('<br>'+'Author : '+result.author.name)
	            $('#parameter_blank').append('<br>'+'Email : '+result.author.email)
            }
          }
        });
      });
    </script>

    




